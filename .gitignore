    -- Invisible Fe Script (embedded)
    local Player = game.Players.LocalPlayer
    local Character = Player.Character or Player.CharacterAdded:Wait()
    local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
    local Humanoid = Character:WaitForChild("Humanoid")

    local invisibleGui = Instance.new("ScreenGui")
    invisibleGui.Name = "InvisibleGui"
    invisibleGui.Parent = Player:WaitForChild("PlayerGui")
    invisibleGui.ResetOnSpawn = false

    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 100, 0, 50)
    toggleButton.Position = UDim2.new(0.5, -50, 0.5, -25)
    toggleButton.BackgroundColor3 = Color3.new(0.7, 0.7, 0.7)
    toggleButton.Text = "Invisible OFF"
    toggleButton.TextColor3 = Color3.new(1, 1, 1)
    toggleButton.Parent = invisibleGui
    toggleButton.Draggable = true

    local isInvisible = false
    local originalTransparency = {}

    local function setTransparency(trans)
        for _, part in pairs(Character:GetDescendants()) do
            if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                if not originalTransparency[part] then
                    originalTransparency[part] = part.Transparency
                end
                part.Transparency = trans
            elseif part:IsA("Decal") then
                if not originalTransparency[part] then
                    originalTransparency[part] = part.Transparency
                end
                part.Transparency = trans
            end
        end
    end

    toggleButton.MouseButton1Click:connect(function()
        isInvisible = not isInvisible
        if isInvisible then
            toggleButton.Text = "Invisible ON"
            toggleButton.BackgroundColor3 = Color3.new(0, 1, 0)
            setTransparency(1)
            HumanoidRootPart.CanCollide = false
        else
            toggleButton.Text = "Invisible OFF"
            toggleButton.BackgroundColor3 = Color3.new(0.7, 0.7, 0.7)
            for part, trans in pairs(originalTransparency) do
                part.Transparency = trans
            end
            HumanoidRootPart.CanCollide = true
            originalTransparency = {}
        end
    end)

    print("Invisible Fe zapushchen, pizdec!")
end)

