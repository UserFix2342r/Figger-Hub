local espButton = Instance.new("TextButton")
espButton.Size = UDim2.new(0, 160, 0, 50)
espButton.Position = UDim2.new(0, 10, 0, 10)
espButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
espButton.Text = "Universal ESP"
espButton.TextColor3 = Color3.fromRGB(255, 255, 255)
espButton.TextSize = 18
espButton.Font = Enum.Font.Gotham
espButton.Parent = aimFrame
local espCorner = Instance.new("UICorner")
espCorner.CornerRadius = UDim.new(0, 5)
espCorner.Parent = espButton
espButton.MouseButton1Click:Connect(function()
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local BOX_COLOR = Color3.fromRGB(255, 0, 0)
    local BOX_TRANSPARENCY = 0.1
    local NAME_COLOR = Color3.fromRGB(255, 255, 255)
    local NAME_TRANSPARENCY = 0.5
    local NAME_SIZE = UDim2.new(0, 80, 0, 20)
    local TEAM_CHECK = false
    local esp_objects = {}

    local function createESP(player)
        if player.Character and player ~= Players.LocalPlayer then
            local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                local box = Instance.new("BoxHandleAdornment")
                box.Size = Vector3.new(4, 6, 4)
                box.Color3 = BOX_COLOR
                box.Transparency = BOX_TRANSPARENCY
                box.AlwaysOnTop = true
                box.Adornee = humanoidRootPart
                box.ZIndex = 10
                box.Parent = game.CoreGui

                local billboard = Instance.new("BillboardGui")
                billboard.Size = NAME_SIZE
                billboard.StudsOffset = Vector3.new(0, 3, 0)
                billboard.Adornee = humanoidRootPart
                billboard.AlwaysOnTop = true
                billboard.Parent = game.CoreGui

                local nameLabel = Instance.new("TextLabel")
                nameLabel.Size = UDim2.new(1, 0, 1, 0)
                nameLabel.BackgroundTransparency = 1
                nameLabel.Text = player.Name
                nameLabel.TextColor3 = NAME_COLOR
                nameLabel.TextTransparency = NAME_TRANSPARENCY
                nameLabel.TextScaled = true
                nameLabel.Font = Enum.Font.SourceSansBold
                nameLabel.Parent = billboard

                esp_objects[player] = {box = box, billboard = billboard}
            end
        end
    end

    local function removeESP(player)
        if esp_objects[player] then
            if esp_objects[player].box then
                esp_objects[player].box:Destroy()
            end
            if esp_objects[player].billboard then
                esp_objects[player].billboard:Destroy()
            end
            esp_objects[player] = nil
        end
    end

    local function updateESP()
        for _, player in ipairs(Players:GetPlayers()) do
            if player.Character and player ~= Players.LocalPlayer then
                if TEAM_CHECK and player.Team == Players.LocalPlayer.Team then
                    removeESP(player)
                else
                    createESP(player)
                end
            else
                removeESP(player)
            end
        end
    end

    Players.PlayerAdded:Connect(function(player)
        player.CharacterAdded:Connect(function()
            createESP(player)
        end)
    end)

    Players.PlayerRemoving:Connect(function(player)
        removeESP(player)
    end)

    RunService.RenderStepped:Connect(updateESP)
    print("Universal ESP запущен, пиздец!")
end)

