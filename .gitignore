local antiBangButton = Instance.new("TextButton")
antiBangButton.Size = UDim2.new(0, 160, 0, 50)
antiBangButton.Position = UDim2.new(0, 180, 0, 10)
antiBangButton.BackgroundColor3 = Color3.fromRGB(0, 100, 0)
